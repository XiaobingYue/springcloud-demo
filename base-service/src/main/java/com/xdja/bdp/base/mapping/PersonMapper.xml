<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xdja.bdp.base.dao.PersonMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xdja.bdp.base.domain.Person">
        <id column="c_id" property="id" />
        <result column="c_name" property="name" />
        <result column="c_identifier" property="identifier" />
        <result column="n_gender" property="gender" />
        <result column="c_email" property="email" />
        <result column="c_telephone" property="telephone" />
        <result column="n_last_update_time" property="lastUpdateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        c_id, c_name, c_identifier, n_gender, c_email, c_telephone, n_last_update_time
    </sql>

</mapper>
